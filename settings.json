{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.detectIndentation": false,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'Source Code Pro', Consolas, 'Courier New', Menlo, Monaco, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "mouseover",
    "editor.multiCursorModifier": "alt",
    "editor.renderWhitespace": "none",
    "editor.smoothScrolling": true,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "off",
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.decorations.enabled": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": true,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.gitExplorer.enabled": true,
    "gitlens.hovers.enabled": false,
    "gitlens.keymap": "chorded",
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.NoBrowser": true,
    "liveServer.settings.wait": 1000,
    "markdown.preview.fontFamily": "Nunito",
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "prettier.printWidth": 100,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintArgs": [
        "--disable=C0111, C0103, R0903"
    ],
    "python.pythonPath": "/Library/Frameworks/Python.framework/Versions/3.6/bin",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Solarized Light",
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "file-icons",
    "workbench.startupEditor": "none",
    "zenMode.hideTabs": false,
    "[html]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 4,
    },
    "editor.tokenColorCustomizations": {
        "[Solarized Light]": {
            "textMateRules": [
                {
                    "scope": "variable.other.object, variable.other.readwrite, variable.other.property, variable.other.object.property",
                    "settings": {
                        "foreground": "#504a72"
                    }
                },
                {
                    "scope": "variable.parameter",
                    "settings": {
                        "foreground": "#000000",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "entity.name.function, meta.object-literal.key, support.function, support.variable",
                    "settings": {
                        "foreground": "#26718f"
                    }
                },
                {
                    "scope": "entity.name.function, support.function",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "entity.name.type",
                    "settings": {
                        "foreground": "#504a72",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "variable.language.this",
                    "settings": {
                        "foreground": "#adadad",
                    }
                },
                {
                    "scope": "support.class, support.type.object",
                    "settings": {
                        "foreground": "#926031",
                    }
                },
                {
                    "scope": "keyword.control",
                    "settings": {
                        //"foreground": "#f14a66"
                        "foreground": "#3596ce",
                        //"foreground": "#439475",
                        "fontStyle": "italic"
                    }
                },
            ]
        },
    },
    "eslint.validate": [
        "html",
        "javascript",
        "javascriptreact",
        "vue",
    ],
}
