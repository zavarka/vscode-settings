{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'Consolas','Courier New', Menlo, Monaco, 'Source Code Pro', monospace",
    "editor.fontLigatures": true,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "mouseover",
    "editor.renderWhitespace": "boundary",
    "editor.smoothScrolling": true,
    "explorer.decorations.colors": false,
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "git.decorations.enabled": true,
    "gitlens.blame.line.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": true,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.gitExplorer.enabled": true,
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "python.linting.flake8Enabled": true,
    "python.linting.pylintArgs": [ "--disable=C0111, C0103, R0903" ],
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Monokai Dimmed",
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.showTabs": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "none",
}
